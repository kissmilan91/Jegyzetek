<Window x:Class="_20220216_Papirgyujtes.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_20220216_Papirgyujtes"
        mc:Ignorable="d"
        Title="Papírgyűjtés" Height="450" Width="500" FontSize="16">
    <DockPanel>
        <Menu DockPanel.Dock="Top" Padding="0,5">
            <MenuItem x:Name="miIskolaStat" Header="Iskolai statisztika" Click="miIskolaStat_Click" />
            <MenuItem x:Name="miOsztalyStat" Header="Osztály statisztika" Click="miOsztalyStat_Click" />
            <MenuItem Name="miUjLeadas" Header="Leadott mennyiségek" Click="miUjLeadas_Click" />
        </Menu>
        <Frame x:Name="frmContainer" NavigationUIVisibility="Hidden" />
    </DockPanel>
</Window>
